{
    "manifest_version": 3,
    "name": "Hello Extensions of the world!",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "download (2).png"
    },
    "host_permissions": [
        "http://example.com/",
        "https://canvas.wpi.edu/*"
    ],
    "permissions": [
        "downloads",
        "tabs"
    ]
}